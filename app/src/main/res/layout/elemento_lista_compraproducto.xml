<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/lstCodComPro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstCodComPro"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstProComPro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstProComPro"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstProvComPro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstProvComPro"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstCantComPro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstCantComPro"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstPreCProd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstPreCProd"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstTotComPro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstTotComPro"
            android:textColor="@color/black"
            android:textStyle="bold" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>