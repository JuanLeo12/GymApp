                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="Antebrazo Izq:"
                android:textAlignment="textEnd" />

            <EditText
                android:id="@+id/txtAISegFis"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="numberDecimal"
                android:maxLength="4" />

            <TextView
                android:id="@+id/textView106"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="Antebrazo Der:"
                android:textAlignment="textEnd" />

            <EditText
                android:id="@+id/txtADSegFis"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="numberDecimal"
                android:maxLength="4" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView107"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Gluteos:"
                android:textAlignment="textEnd" />

            <EditText
                android:id="@+id/txtGlutSegFis"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="numberDecimal"
                android:maxLength="5" />

            <TextView
                android:id="@+id/textView108"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Muslo Izq:"
                android:textAlignment="textEnd" />

            <EditText
                android:id="@+id/txtMISegFis"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="numberDecimal"
                android:maxLength="4" />

            <TextView
                android:id="@+id/textView109"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Muslo Der:"
                android:textAlignment="textEnd" />

            <EditText
                android:id="@+id/txtMDSegFis"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="numberDecimal"
                android:maxLength="4" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView110"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Pantorrilla Izq:"
                android:textAlignment="textEnd" />

            <EditText
                android:id="@+id/txtPISegFis"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="numberDecimal"
                android:maxLength="4" />

