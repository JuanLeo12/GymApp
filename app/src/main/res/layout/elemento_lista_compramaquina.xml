<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/lstCodComMaq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstCodComMaq"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstMaqCom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstMaqCom"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstProvComMaq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstProvComMaq"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstCanComMaq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstCanComMaq"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstPreCComMaq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstPreCComMaq"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstTotComMaq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstTotComMaq"
            android:textColor="@color/black"
            android:textStyle="bold" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>