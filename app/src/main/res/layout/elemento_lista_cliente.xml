<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/lstCodCli"
            android:layout_width="10dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstCodCli"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstNomCli"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstNomCli"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstApPatCli"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstApPatCli"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstApMatCli"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstApMatCli"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstTelfCli"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstTelfCli"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstCorrCli"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstCorrCli"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstDirCli"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:text="lstDirCli"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstGenCli"
            android:layout_width="10dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstGenCli"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstMemCli"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstMemCli"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lstEstCli"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="lstEstCli"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="bold" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>